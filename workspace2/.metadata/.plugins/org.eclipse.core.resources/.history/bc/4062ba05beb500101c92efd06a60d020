#include "conf.h"

void confRCC(void){
                /*    IOB,    IOA,   AF*/
    RCC->AHB1ENR |= (1<<0); // GPIOA ADC1 PA0
    RCC->APB2ENR|=(1<<7); //PRENDEMOS ADC1
    RCC->APB2ENR |= (1<<0); //TIM1

}
void confGPIO(void){
	GPIOA->MODER |= (3<<0);
}


void confSPI(void){


}

void config(void){
    confRCC();
    confGPIO();
    confSPI();
}
