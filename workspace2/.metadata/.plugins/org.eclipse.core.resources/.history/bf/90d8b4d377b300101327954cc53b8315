{
  HAL_Init();
  SystemClock_Config();
  MX_GPIO_Init();
  MX_SPI1_Init();
  init();
  ST7735_FillScreen(ST7735_BLACK);
	ST7735_DrawImage(0,0,160,128,TestImage);

