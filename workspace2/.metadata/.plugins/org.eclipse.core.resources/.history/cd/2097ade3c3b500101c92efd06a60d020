// --------- Function ---------
void confRCC(void);
void confGPIO(void);
// ---------- Class ----------
// -------- Variables --------
// ----------- Main -----------
uint8_t SPI_Send_Data(uint8_t);

int main(void){
	config();
	while(1){
	}


}

uint8_t SPI_Send_Data(uint8_t dataTX){
	while(!(SPI1->SR & SPI_SR_TXE));
	SPI->DR = dataTX;
	while(!(SPI1->SR & SPI_SR_RXNE));
	uint8_t dataRX;
	SPI->DR = dataRX;
}
